/* 个人信息区开始 */
.login{
   height: 400rpx;
}

.phone{
   display: flex;
   align-items: center;
   justify-content: center;
   margin-top: 60rpx;
}
.name{
   display: flex;
   align-items: center;
   justify-content: center;
   font-size: 32rpx;
   color: #f08115;
   margin-bottom: 20rpx;
}
.btn{
   display: flex;
   align-items: center;
   justify-content: center;
   margin-bottom: 10rpx;
}
.box{
   margin-top: 120rpx;
}

.background{
   width: 100%;
   height: 520rpx;
   position: fixed;
   background-size: 100% 100%;
   z-index: -1;
}
.button1{
   height: 88rpx;
   padding: 0;
   background: #ffffff;
   border-bottom:1rpx solid #f5f3f3 ;
}
.big{
   width: 25rpx;
   height: 25rpx;
   padding: 4rpx;
   
}
.back .feedback{
   display: flex;
   align-items: center;
   justify-content: space-between;
   background: #ffffff;
   padding:0 30rpx;
   margin-top: 10rpx;
   

}
.style1{
   padding-left: 2px;
   font-size:14px ;
   color: #535353;
}
.style2{
   color: #a19f9f;
}




